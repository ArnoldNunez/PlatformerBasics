        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Set movement
hsp = dir * movespeed;
vsp += grav;


/// Horizontal collision
if (place_meeting(x+hsp, y, obj_wall)) /// if next move causes collision
{
    while(!place_meeting(x+sign(hsp), y, obj_wall))
    {
        /// Move us 1 px a frame towards the collision until collision occurs
        /// Sign returns 1 or -1 depending on whether var is pos or neg
        x += sign(hsp);
    }
    hsp = 0;
    
    dir *= -1;  /// Change the direction
}

x += hsp;   /// If there is a collision it will be 0

/// Vertical collision
if (place_meeting(x, y+vsp, obj_wall))
{
    while(!place_meeting(x, y+sign(vsp), obj_wall))
    {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;

/// Enemy collision
if (place_meeting(x, y, obj_player))
{
    if (obj_player.y &lt; y - 16) /// If colliding from top
    {
        with (obj_player) vsp = -jumpspeed; /// Bounce
        instance_destroy();
    }
    else
    {
        /// Player gets hit
        game_restart();
    }
}
</string>
