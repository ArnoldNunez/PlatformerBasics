    hsp = 0;
}

x += hsp;   /// If there is a collision it will be 0

/// Vertical collision
if (place_meeting(x, y+vsp, obj_wall))
{
    while(!place_meeting(x, y+sign(vsp), obj_wall))
    {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;
</string>
